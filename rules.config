block@scope -> statement block | statement
statement -> declaration | assignment | funccall | conditional | jump | label
assignment@assign -> IDENTIFIER@var OPERATOR("=") expression@exp SEMICOLON
funccall@call -> IDENTIFIER@func LPAREN argument@args RPAREN SEMICOLON
argument -> expression COMMA argument | expression
conditional@cond -> "if" LPAREN expression@cond RPAREN LBRACE block@scope RBRACE
declaration@dec -> IDENTIFIER@type IDENTIFIER@name OPERATOR("=") expression@exp SEMICOLON
jump@jmp -> "goto" IDENTIFIER@jmp SEMICOLON
label@label -> IDENTIFIER@name COLON
expression@exp -> CONST@val OPERATOR@op expression | IDENTIFIER@var OPERATOR@op expression | CONST@val | IDENTIFIER@var
